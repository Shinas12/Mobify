

              <!-- Table with hoverable rows -->
              <div id="main">
              <table class="table table-hover">
                <thead>
                  <tr>
                   
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                     <th scope="col">ProductName</th>
                    <th scope="col">Date</th>
                  </tr>
                </thead>
                <tbody>
                    {{#each AdminorderSummarry}}
                  <tr>
                    <th scope="row">{{this.DeliveryDetails.CustomerName}}</th>
                    <td>{{this.DeliveryDetails.CustomerEmail}}</td>
                    <td>{{this.DeliveryDetails.CustomerPhone}}</td>
                    <td>{{this.productdetails.productname}}</td>
                    <td>{{this.date}}</td>
                    <td>
                       <a href="/admin/viewMore/{{this.productdetails._id}}" class="btn btn-success" >More</a> 
                    </td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
              <!-- End Table with hoverable rows -->
            </div>
            